#include <iostream>
#include <iomanip>
#include <math.h>
#include<conio.h>

using namespace std;


int main()  
{
    int a [] = { 32, 33, 32, 34, 35, 36, 38};
    int b [] = { 23, 24, 26, 22, 29, 27, 23};
    int c [] = { 30, 29, 26, 28, 29, 27, 29};

    for (int i=0;i<=6;i++)
     cout << "Province A, Day " << i+1 << ": " << a[i] << "\n"; 
     
    for (int i=0;i<=6;i++) 
     cout << "Province B, Day " << i+1 << ": " << b[i] << "\n";
     
    for (int i=0;i<=6;i++)
     cout << "Province C, Day " << i+1 << ": " << c[i] << "\n";
	
	getch();
    return 0;
}